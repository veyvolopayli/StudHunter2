<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LeaveReviewNotificationView">
        <attr name="leaveReviewCardColor" format="color" />
        <attr name="leaveReviewCardRadius" format="dimension" />
    </declare-styleable>
    <declare-styleable name="LgbtqButtonView">
        <attr name="buttonText" format="string" />
        <attr name="buttonType" format="string" />
        <attr name="isActive" format="boolean" />
    </declare-styleable>
    <declare-styleable name="HeartView">
        <attr name="isChecked" format="boolean" />
    </declare-styleable>
    <declare-styleable name="BubbleTextCellView">
        <attr name="cellIsActive" format="boolean" />
        <attr name="cellText" format="string" />
    </declare-styleable>
    <declare-styleable name="RatingsCustomView">
        <attr name="activeRatingCell" format="enum">
            <enum name="any" value="0" />
            <enum name="one" value="1" />
            <enum name="two" value="2" />
            <enum name="three" value="3" />
            <enum name="four" value="4" />
            <enum name="five" value="5" />
        </attr>
    </declare-styleable>
</resources>